@using ViewRes
@model List<Webmall.Model.Entities.Catalog.WareProperty>
@{
    Layout = null;
}
@if (Model.Any())
{

    foreach (var el in Model)
    {
        <div class="product__param">
            <dt class="product__property">@el.Name</dt>
            <dd class="product__value">@el.Value</dd>
        </div>
    }
}
else
{
    <div class="product__param">@SharedResources.NoData</div>
}
